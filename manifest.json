{
    "manifest_version": 3,
    "name": "Auto đăng kí học phần HAUI",
    "description": "Giúp bạn đăng kí học phần nhanh hơn!",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
        {
          "js": ["main.js"],
          "matches": [
            "https://sv.haui.edu.vn/*","https://haui.edu.vn/*"
          ]
        }
    ]
  }